{
  "name": "OLSKRollupScaffold",
  "main": "main.js",
  "bin": {
    "olsk-rollup": "./package-bin.js",
    "olsk-rollup-watch": "./package-bin.js"
  },
  "dependencies": {
    "rollup": "latest",
    
    "//": "SVELTE",
    "svelte": "latest",
    "rollup-plugin-svelte": "6.x",
    
    "//": "NPM MODULES",
    "rollup-plugin-commonjs": "latest",
    "rollup-plugin-node-resolve": "latest",    

    "//": "LOCALIZE",
    "OLSKRollupPluginLocalize": "olsk/OLSKRollupPluginLocalize",

    "//": "SWAP",
    "OLSKRollupPluginSwap": "olsk/OLSKRollupPluginSwap",

    "//": "SVG IMPORT",
    "rollup-plugin-svg-import": "latest",

    "//": "CSS PREPROCESSING",
    "svelte-preprocess": "latest",
    "postcss": "latest",

    "//": "MINIFY",
    "rollup-plugin-terser": "latest",

    "//": "LINT",
    "eslint-plugin-svelte3": "latest",

    "//": "FILE ACCESS",
    "glob": "latest",

    "//": "ENV GUARD",
    "OLSKEnv": "olsk/OLSKEnv",
    
    "//": "I sacrifice myself for the commas of others"
  }
}
